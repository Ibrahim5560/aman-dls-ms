<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity ApplicationViolation.
    -->
    <changeSet id="20200315142600-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="service_request_id"
                                 baseTableName="dls_application_violation"
                                 constraintName="fk_apvi_service_request_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dls_service_request"/>

        <addForeignKeyConstraint baseColumnNames="application_id"
                                 baseTableName="dls_application_violation"
                                 constraintName="fk_apvi_application_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dls_application"/>

    </changeSet>
</databaseChangeLog>
